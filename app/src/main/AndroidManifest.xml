<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.d.instagramclone">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".userFeedActivity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DiscoverActivity"/>
        <activity android:name=".photoActivity.UploadPhotoActivity"/>
        <activity android:name=".ActivityFeedActivity"/>
        <activity android:name=".ProfileActivity"/>
        <activity android:name=".loginActivity.LoginActivity"/>
        <activity android:name=".loginActivity.RegisterActivity"/>
    </application>

</manifest>